{
  "name": "wut",
  "description": "Web Update Tool",
  "type": "module",
  "dependencies": {
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/node": "^24.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "scripts": {
    "hot": "deno run --allow-all --watch src/srv.ts",
    "up": "docker compose --file docker/srv.yaml up --detach --pull always",
    "down": "docker compose --file docker/srv.yaml down"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
